<%= simple_form_for([ @kitchen, @booking ]) do |form| %>
  <%= form.input :status, as: :string %>
  <%= form.input :user_id, as: :hidden, input_html: {value: @user_id } %>
  <%= form.input :kitchen_id, as: :hidden, input_html: {value: @kitchen_id } %>

  <%= form.button :submit %>
<% end %>



